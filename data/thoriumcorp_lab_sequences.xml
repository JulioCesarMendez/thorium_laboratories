<odoo>
    <data noupdate="1">
        <!-- Sequences for lab test report -->
        <record id="seq_thoriumcorp_lab_test" model="ir.sequence">
            <field name="name">Lab Test</field>
            <field name="code">thoriumcorp.lab.test.result</field>
            <field name="prefix">TEST</field>
            <field name="padding">3</field>
        </record>
        <!-- Sequences for lab request -->
        <record id="seq_thoriumcorp_lab_request" model="ir.sequence">
            <field name="name">Lab Request</field>
            <field name="code">thoriumcorp.lab.test.requests</field>
            <field name="prefix">LRQ</field>
            <field name="padding">3</field>
        </record>
    </data>
</odoo>
